#Add an item

#program

a=(4, 6, 2, 8, 3, 1)
print(a)
a=a+(9,)
print(a)
a=a[:5]+(15, 20, 25)+a[:5]
print(a)
b=list(a)
b.append(30)
a=tuple(b)
print(a)

#Length of a tuple

#program

m=('c', 'y', 'f', 'u', 'n', 'o')
print(m)
print(len(m))

#Reverse a Tuple

#program

a=('o', 'n', 'u', 'f', 'y', 'c')
print(a[::-1])
print(a)

#Repeated element in a tuple

#program

a=(2, 4, 5, 6, 2, 3, 4, 4, 7)
b=int(input())
print(a)
print(a.count(b))
